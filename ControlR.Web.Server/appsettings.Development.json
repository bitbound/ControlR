{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "ControlR.Server.Auth.DigitalSignatureAuthenticationHandler": "Warning"
      }
    }
  },

  "POSTGRES_USER": "postgres",
  "POSTGRES_PASSWORD": "password",
  "POSTGRES_HOST": "localhost",

  "AzureMonitor": {
    "ConnectionString": ""
  },
  "OTLP_ENDPOINT_URL": "http://localhost:18889",

  "AppOptions": {
    "ServerBaseUri": "https://localhost:7033",
    "EnablePublicRegistration": false,
    "UseRedisBackplane": false,
    "UseExternalWebSocketBridge": false,
    "LogRetentionDays": 7,
    "ExternalWebSocketHosts": []
  }
}