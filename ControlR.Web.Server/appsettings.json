{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "ControlR.Server.Auth.DigitalSignatureAuthenticationHandler": "Warning"
      }
    }
  },
  "AllowedHosts": "*",

  "AzureMonitor": {
    "ConnectionString": ""
  },
  "OTLP_ENDPOINT_URL": "",

  "ApplicationOptions": {
    "EnablePublicRegistration": false,
    "UseRedisBackplane": false,
    "UseExternalWebSocketBridge": false,
    "LogRetentionDays": 7,
    "ExternalWebSocketHosts": [

    ]
  }
}