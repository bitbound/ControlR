{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Agent (Run)",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/ControlR.Agent/bin/Debug/net9.0/ControlR.Agent.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ControlR.Agent",
      "launchSettingsProfile": "Run",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "DesktopClient",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/ControlR.DesktopClient/bin/Debug/net9.0/ControlR.DesktopClient.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ControlR.DesktopClient",
      "launchSettingsProfile": "Run",
      "stopAtEntry": false,
      "console": "internalConsole",
    },
    {
      "name": "Server (https)",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/ControlR.Web.Server/bin/Debug/net9.0/ControlR.Web.Server.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ControlR.Web.Server",
      "launchSettingsProfile": "https",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Server (wasm debug)",
      "type": "blazorwasm",
      "request": "launch",
      "browser": "edge",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "hosted": true,
      "program": "${workspaceFolder}/ControlR.Web.Server/bin/Debug/net9.0/ControlR.Web.Server.dll",
      "cwd": "${workspaceFolder}/ControlR.Web.Server",
    },
    {
      "name": "LoadTester",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/Tests/ControlR.Agent.LoadTester/bin/Debug/net9.0/ControlR.Agent.LoadTester.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Tests/ControlR.Agent.LoadTester",
      "stopAtEntry": false,
      "console": "internalConsole"
    }
  ],
  "compounds": [
    {
      "name": "Full Stack",
      "preLaunchTask": "build + compose",
      "configurations": [
        "Server (https)",
        "Agent (Run)",
        "DesktopClient"
      ],
      "stopAll": true,
      "presentation": {
        "hidden": false,
        "group": "Compounds",
        "order": 0
      }
    },
      {
      "name": "Full Stack (wasm debug)",
      "preLaunchTask": "build + compose",
      "configurations": [
        "Server (wasm debug)",
        "Agent (Run)",
        "DesktopClient"
      ],
      "stopAll": true,
      "presentation": {
        "hidden": false,
        "group": "Compounds",
        "order": 1
      }
    },
    {
      "name": "Load Test",
      "preLaunchTask": "build + compose",
      "configurations": [
        "Server (https)",
        "LoadTester"
      ],
      "stopAll": true,
      "presentation": {
        "hidden": false,
        "group": "Compounds",
        "order": 2
      }
    },
    {
      "name": "Backend",
      "preLaunchTask": "build + compose",
      "configurations": [
        "Server (https)"
      ],
      "stopAll": true,
      "presentation": {
        "hidden": false,
        "group": "Compounds",
        "order": 3
      }
    }
  ]
}