{
  "profiles": {
    "Run": {
      "commandName": "Project",
      "commandLineArgs": "run -i localhost",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development",
        "AppOptions__ServerUri": "http://localhost:5120"
      }
    },
    "Run (Native Debug)": {
      "commandName": "Project",
      "commandLineArgs": "run",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development",
        "AppOptions__ServerUri": "http://localhost:5120"
      },
      "nativeDebugging": true
    },
    "Install": {
      "commandName": "Project",
      "commandLineArgs": "install -i localhost -s http://localhost:5120",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    }
  }
}