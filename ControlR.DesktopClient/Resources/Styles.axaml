	<!-- Variant base classes -->
	<Style Selector=":is(ContentControl).VariantFilled">
		<Setter Property="BorderThickness" Value="0" />
	</Style>

	<Style Selector=":is(ContentControl).VariantOutlined">
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderThickness" Value="1" />
	</Style>

	<Style Selector=":is(ContentControl).VariantText">
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderThickness" Value="0" />
	</Style>

