	<Design.PreviewWith>
		<Border Padding="20">
			<!-- Add Controls for Previewer Here -->
		</Border>
	</Design.PreviewWith>

	<Style Selector="TextBlock.h1">
		<Setter Property="FontSize" Value="24" />
		<Setter Property="FontWeight" Value="Bold" />
	</Style>

	<Style Selector="TextBlock.h2">
		<Setter Property="FontSize" Value="18" />
		<Setter Property="FontWeight" Value="Bold" />
	</Style>

	<Style Selector="TextBlock.Small">
		<Setter Property="FontSize" Value="10" />
	</Style>

	<Style Selector="TextBlock.Primary">
		<Setter Property="Foreground" Value="{DynamicResource PrimaryColor}" />
	</Style>

	<Style Selector="TextBlock.Info">
		<Setter Property="Foreground" Value="{DynamicResource InfoColor}" />
	</Style>

	<Style Selector="TextBlock.Error">
		<Setter Property="Foreground" Value="{DynamicResource ErrorColor}" />
	</Style>

	<Style Selector="Border.Card">
		<Setter Property="Background" Value="{DynamicResource SurfaceBrush}" />
		<Setter Property="BorderBrush" Value="{DynamicResource DividerBrush}" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="CornerRadius" Value="8" />
		<Setter Property="Padding" Value="16" />
	</Style>

	<!-- Color Styles -->
	<Style Selector=":is(ContentControl).Info">
		<Setter Property="Background" Value="{DynamicResource InfoColor}" />
	</Style>

	<Style Selector=":is(ContentControl).Primary">
